<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userInterfaceLaag.DocentAanmakenController">
   <children>
      <HBox layoutX="75.0" layoutY="81.0" prefHeight="20.0" prefWidth="249.0" spacing="5.0">
         <children>
            <Label prefHeight="17.0" prefWidth="104.0" text="Docentnaam:" />
            <TextField fx:id="docentnaam" prefHeight="25.0" prefWidth="176.0" />
         </children>
      </HBox>
      <HBox layoutX="75.0" layoutY="114.0" prefHeight="25.0" prefWidth="249.0" spacing="7.0">
         <children>
            <Label prefHeight="17.0" prefWidth="91.0" text="Wachtwoord:" />
            <TextField fx:id="wachtwoord" prefHeight="25.0" prefWidth="165.0" />
         </children>
      </HBox>
      <Label layoutX="75.0" layoutY="28.0" text="Hier kunt u een nieuwe docent account aanmaken" />
      <Button layoutX="75.0" layoutY="161.0" mnemonicParsing="false" onMousePressed="#opslaan" text="Opslaan" />
      <Button layoutX="148.0" layoutY="161.0" mnemonicParsing="false" onMousePressed="#cancelButton" text="Cancel" />
      <Label fx:id="errorLocatie" layoutX="75.0" layoutY="200.0" textFill="RED" />
   </children>
</AnchorPane>
