<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userInterfaceLaag.InloggenController">
   <children>
      <ImageView fitHeight="129.0" fitWidth="427.0" layoutX="83.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Pictures/HU%20Logo%20Groot.png" />
         </image>
      </ImageView>
      <Label fx:id="foutmeldingLabel" layoutX="105.0" layoutY="322.0" prefHeight="17.0" prefWidth="126.0" textFill="#ee0101" />
      <GridPane alignment="CENTER_RIGHT" layoutX="90.0" layoutY="181.0" prefHeight="141.0" prefWidth="421.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="179.0" minWidth="10.0" prefWidth="161.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="226.0" minWidth="10.0" prefWidth="206.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="gebruikersnaamInput" GridPane.columnIndex="1">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></TextField>
            <Label text="Gebruikersnaam:" />
            <Label text="Wachtwoord:" GridPane.rowIndex="1" />
            <PasswordField fx:id="wachtwoordInput" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></PasswordField>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#inloggen" prefHeight="25.0" prefWidth="66.0" text="Log in" GridPane.rowIndex="2" />
            <Hyperlink alignment="CENTER_RIGHT" onMousePressed="#tempAccountAanmaken" text="Wachtwoord vergeten" textAlignment="RIGHT" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
